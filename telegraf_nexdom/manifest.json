{
  "name": "Telegraf Nexdom Collector",
  "slug": "telegraf_nexdom",
  "version": "1.2.8",
  "stage": "stable",
  "description": "Cliente Telegraf para HAOS que envía métricas MQTT locales hacia InfluxDB central de Nexdom.",
  "arch": [
    "amd64",
    "aarch64"
  ],
  "startup": "services",
  "boot": "auto",
  "panel_icon": "mdi:gauge",
  "panel_title": "Nexdom Metrics",
  "url": "https://nexdom.mx",
  "image": "docker.io/chekosg/telegraf_nexdom-{arch}",
  "map": [
    "config:rw"
  ],
  "options": {
    "interval_seconds": 300,
    "influx_url": "https://influx.nexdom.mx",
    "influx_token": "",
    "influx_org": "Nexdom",
    "influx_bucket": "haos",
    "mqtt_host": "core-mosquitto",
    "mqtt_port": 1883,
    "mqtt_user": "telegraf",
    "mqtt_password": "telegraf123",
    "client_id": "haos_cliente"
  },
  "schema": {
    "interval_seconds": "int",
    "influx_url": "str",
    "influx_token": "str",
    "influx_org": "str",
    "influx_bucket": "str",
    "mqtt_host": "str",
    "mqtt_port": "int",
    "mqtt_user": "str",
    "mqtt_password": "str",
    "client_id": "str"
  },
  "logo": "logo.png",
  "icon": "icon.png"
}
